{
  "cloud": {
    "ios": {
      "app": {
        "bundleId": "<<bundle_id>>",
        "path": "./builds/<<_file_with_path>>"
      },
      "targets": [
        {
          "deviceName": "iPhone 11",
          "platformVersion": "16"
        }
      ],
      "server": {
        "url": "http://hub.browserstack.com/",
        "userName": "<<user_name>>",
        "accessKey": "<<access_key>>",
        "uploadApi" : "https://api-cloud.browserstack.com/app-automate/upload"
      },
      "project" : {
        "AUTName": "App Test Automation",
        "AUTBuild": "App {{build}}",
        "testName": "iOS App Test {{timestamp}}"
      }
    }
  },
  "local": {
    "ios": {
      "app": {
        "bundleId": "<<bundle_id>>",
        "path": "./builds/ios/<<_file_with_path>>"
      },
      "targets": [
        {
          "deviceName": "iPad-9",
          "platformVersion": "16"
        }
      ],
      "server": {
        "url": "http://127.0.0.1:4723/wd/hub",
        "automationName": "XCUITest",
        "launchTimeout": 120000,
        "commandTimeout": 12000
      }
    }
  }
}